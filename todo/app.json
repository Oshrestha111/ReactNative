{
  "expo": {
    "name": "My Todo",
    "slug": "my-todo",
    "description": "This is a todo list app which stores your todos for the day.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/todo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/todo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
